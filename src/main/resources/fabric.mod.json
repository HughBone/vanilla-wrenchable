{
  "schemaVersion": 1,
  "id": "wrenchable",
  "version": "${version}",

  "name": "Wrenchable",
  "description": "A library designed for allowing interactivity of wrenches between various mods.",
  "authors": [
    "HughBone"
  ],
  "contact": {
    "sources": "https://github.com/HughBone/vanilla-wrenchable",
	"issues": "https://github.com/HughBone/vanilla-wrenchable/issues"
  },

  "license": "MIT",
  "icon": "assets/wrenchable/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.hughbone.vanilla_wrenchable.WrenchableMain"
    ]
  },
  "mixins": [
  ],

  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric": "*"
  },
  "custom": {
    "modmenu:api": true,
    "modmenu:clientsideOnly": false
  }
}
